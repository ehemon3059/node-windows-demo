<service>
	<id>nodewindowsdemoservice.exe</id>
	<name>NodeWindowsDemoService</name>
	<description>Demo Node service using node-windows</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>--harmony</argument>
	<argument>H:\Desktop-app-Build\node-windows-demo\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>H:\Desktop-app-Build\node-windows-demo\worker.js</argument>
	<argument>--scriptoptions=</argument>
	<argument>--log</argument>
	<argument>NodeWindowsDemoService wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<logmode>rotate</logmode>
	<stoptimeout>30sec</stoptimeout>
	<serviceaccount>
		<domain>EH</domain>
		<user>LocalSystem</user>
		<password></password>
	</serviceaccount>
	<workingdirectory>H:\Desktop-app-Build\node-windows-demo</workingdirectory>
</service>